[user]
  email = lowe.thiderman@gmail.com
  name = Lowe Thiderman
[color]
  ui = auto

[color "diff"]
  plain = normal
  meta = bold
  frag = cyan
  old = bold red
  new = bold green
  commit = bold yellow
  whitespace = normal red
[color "branch"]
  current = bold green
  local = normal
  remote = bold red
  plain = normal
[color "status"]
  header = normal
  added = cyan
  updated = bold green
  changed = bold red
  untracked = bold magenta
  nobranch = bold red
[color "grep"]
  match = bold magenta
[color "interactive"]
  prompt = normal
  header = normal
  help = yellow
  error = bold red

[core]
  editor = emacsclient -nw
  excludesfile = ~/.cvsignore
[push]
	default = tracking
	followTags = true
[url "git@github.com:"]
	insteadOf = https://github.com/
[url "git@lab.unomaly.net:"]
	insteadOf = https://lab.unomaly.net/
[pull]
	rebase = true
[github]
	user = thiderman
[credential "https://lab.unomaly"]
	com = thiderman
