#!/bin/zsh
# xrandr screen switch and rotator

case $1 in
    "laptop")
        xrandr --output DP-1 --off
        xrandr --output eDP-1 --auto
        ;;
    "work")
        xrandr --output DP-1 --auto
        xrandr --output eDP-1 --right-of DP-1
        ;;
esac

keyboard-setup
wp

# killall keynav; keynav &> /dev/null &|
